    "mdbreact": "^5.0.2",
    "modal": "^1.2.0",
    "moment": "^2.29.1",
    "next": "10.0.6",
    "next-compose-plugins": "2.2.1",
    "next-fonts": "1.4.0",
    "next-images": "1.3.1",
    "next-videos": "^1.4.0",
    "node-sass": "4.14.1",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "14.6.3",
    "path": "0.12.7",
    "react": "^17.0.1",
    "react-bootstrap": "^1.6.1",
    "react-chartjs-2": "2.11.1",
    "react-confirm-alert": "^2.7.0",
    "react-copy-to-clipboard": "5.0.3",
